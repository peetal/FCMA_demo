    "        acc_start_index = line.index(': ') + 2\n",
    "        acc_end_index = line.index('\\n')\n",
    "        score = float(line[acc_start_index:acc_end_index])\n",
    "        acc.append(score)\n",
    "\n",
    "    # write a dataframe\n",
    "    \n",
    "    colnames = ['loo_id','clf_acc','top_num']\n",
    "    df = pd.DataFrame(index=range(len(loo_id)), columns=colnames)\n",
