    "    loo_id = []\n",
    "    acc = []\n",
    "    for line in lines: \n",
    "        id_start_index = line.index('_no') + len('_no')\n",
    "        id_end_index = line.index('_result_seq')\n",
    "        id = int(line[id_start_index:id_end_index])\n",
    "        loo_id.append(id)\n",
